    <script>
        function AutoSync(a) {
            var b = new XMLHttpRequest();
            b.open('GET', '@System.Web.Configuration.WebConfigurationManager.AppSettings["ConPath"]' + 'Sync/SyncDb?location=' + a, true);
            b.setRequestHeader('Accept', 'application/json');
            b.onreadystatechange = function () {
                if (b.readyState === 4 && b.status === 200) {
                    alert(b.responseText);
                    var c = new XMLHttpRequest();
                    c.open('GET', '@System.Web.Configuration.WebConfigurationManager.AppSettings["ConPath"]' + 'ChangeRelease/Cleanup', true);
                    c.setRequestHeader('Accept', 'application/json');
                    c.send();
                };
            }; b.send();
        };
        function COFF() {
            var a = "@Session["GradeCode"]";
            var b = "@Session["OtFlag"]";
            var c = "@Session["LOC"]";
            var d = "@Session["CatCode"]";
            if (c === "IPU") { if ((a < "015" || a === "098")) { alert("You are not eligible to get COFF as Per Policy."); } else { window.location.href = "/LeaveApp/COffApplication"; }; }
            else if (c === "NKP" || c === "KJSAW") { if ((a < "015" || a === "098") || b === "True") { alert("You are not eligible to get COFF as Per Policy."); } else { window.location.href = "/LeaveApp/COffApply"; }; }
            else if (c === "BEL") { if ((a < "009" || a === "098") || b === "True") { alert("You are not eligible to get COFF as Per Policy."); } else { window.location.href = "/LeaveApp/COffApply"; }; }
            else if (c === "JFL") { if ((a < "040" || d === "003") || b === "True") { alert("You are not eligible to get COFF as Per Policy."); } else { window.location.href = "/LeaveApp/COffApplyJFL"; }; }
            else if (c === "NSK" && e === "COMP") { if ((a < "010" || a === "200")) { alert("You are not eligible to get COFF as Per Policy."); } else { window.location.href = "/LeaveApp/COffApplication"; }; }
            else if (c === "NSK" && e === "CONSULTANT") { window.location.href = "/LeaveApp/COffApplication"; };
        };
    </script>
