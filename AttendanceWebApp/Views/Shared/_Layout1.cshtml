            if (foo === "NKP") { $('#footext').text("JINDAL SAW LTD. (NKP)"); $('#headtext').text("Employee Self Service Portal - Jindal SAW Ltd."); };
            if (foo === "BEL") { $('#footext').text("JINDAL SAW LTD. (BALLARY)"); $('#headtext').text("Employee Self Service Portal - Jindal SAW Ltd."); };
            if (foo === "JFL") { $('#footext').text("JINDAL SAW LTD. (TEM)"); $('#headtext').text("Employee Self Service Portal - Jindal SAW Ltd."); };
            if (foo === "KJSAW") { $('#footext').text("JINDAL SAW LTD. (KOSI) -"); $('#headtext').text("Employee Self Service Portal - Jindal SAW Ltd."); };
