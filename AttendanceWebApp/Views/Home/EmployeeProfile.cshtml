@{
    ViewBag.Title = "User Profile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/Scripts/Controller/HomeCntrl.js"></script>
<script src="~/Content/DirPAgination/dirPagination.js"></script>
<style type="text/css">
    .TextBoxStyle {
        height: 25px;
    }

    .AddNew {
        float: right;
        position: relative;
    }

    .right-inner-addon {
        position: relative;
    }

        .right-inner-addon input {
            padding-right: 30px;
        }

        .right-inner-addon i {
            position: absolute;
            right: 0px;
            padding: 10px 12px;
            pointer-events: none;
        }
</style>
<div ng-app="myApp" ng-controller="HomeCntrloller">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-12">
                <div class="table-responsive">
                    @* JOB Profile Details *@
                    <div class="panel panel-info">
                        <div class="panel-body">
                            <div ng-init="GetUserInfo()" class="table-responsive">
                                <table class="table table-hover table-bordered table-condensed table-striped" dir-paginate="d in Udata|itemsPerPage:5">
                                    <thead>
                                        <tr>
                                            <td colspan="4" class="info fc-header-center">
                                                <h4>
                                                    <b>Employee Code / SAPID : {{d.empUnqId}}</b>
                                                </h4>
                                            </td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <th colspan="4" style="text-align:center;">
                                                <h4>
                                                    <strong>Job Profile</strong>
                                                </h4>
                                            </th>
                                        </tr>
                                        <tr>
                                            <td style="width:16.6%">
                                                <label>Employee Name</label>
                                            </td>
                                            <td style="width:30%">{{d.empName}}</td>
                                            <td style="width:16.6%">
                                                <label>Father Name</label>
                                            </td>
                                            <td style="width:30%">{{d.fatherName}}</td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <label>Company Name</label>
                                            </td>
                                            <td>{{d.compName}}</td>
                                            <td>
                                                <label>Work Group</label>
                                            </td>
                                            <td>{{d.wrkGrpDesc}}</td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <label>Unit Name</label>
                                            </td>
                                            <td>{{d.unitName}}</td>
                                            <td>
                                                <label>Department Name</label>
                                            </td>
                                            <td>{{d.deptName}}</td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <label>Station Name</label>
                                            </td>
                                            <td>{{d.statName}}</td>
                                            <td>
                                                <label>Category Name</label>
                                            </td>
                                            <td>{{d.catName}}</td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <label>Employee Type Name</label>
                                            </td>
                                            <td>{{d.empTypeName}}</td>
                                            <td>
                                                <label>Grade Name</label>
                                            </td>
                                            <td>{{d.gradeName}}</td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <label>Designation Name</label>
                                            </td>
                                            <td>{{d.desgName}}</td>
                                            <td>
                                                <label>Email Address</label>
                                            </td>
                                            <td>{{d.email}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            <div ng-init="GetUserPerasonalInfo()" class="table-responsive">
                                <table class="table table-hover table-bordered table-condensed table-striped" dir-paginate="pd in Uperdata|itemsPerPage:5">
                                    <thead>
                                        <tr>
                                            <th colspan="4" class="fc-header-center">
                                                <h4>
                                                    <strong>Personal Information</strong>
                                                </h4>
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td style="width:16.6%">
                                                <label>Joining Date</label>
                                            </td>
                                            <td style="width:30%">{{pd.joinDt | date:'dd/MM/yyyy'}}</td>
                                            <td style="width:16.6%">
                                                <label>Birth Date</label>
                                            </td>
                                            <td style="width:30%">{{pd.birthDate | date:'dd/MM/yyyy'}}</td>
                                        </tr>
                                        <tr>
                                            <td>
                                                <label>Contact No.</label>
                                            </td>
                                            <td>{{pd.contactNo}}</td>
                                            <td>
                                                <label>Blood Group </label>
                                            </td>
                                            <td>{{pd.bloodGroup}}</td>
                                        </tr>
                                        @* Address Details *@
                                        <tr>
                                            <th colspan="2" style="text-align:center;">
                                                <h4>
                                                    <b>Permanent Address</b>
                                                </h4>
                                            </th>
                                            <th colspan="2" style="text-align:center;">
                                                <h4>
                                                    <b>Present Address</b>
                                                </h4>
                                            </th>
                                        </tr>
                                        <tr>
                                            @* Parmanent Address *@
                                            <td colspan="2">
                                                <table class="table table-hover table-bordered table-condensed table-striped">
                                                    <tr>
                                                        <td style="width:35%;">
                                                            <label>Address 1</label>
                                                        </td>
                                                        <td style="width:65%;">{{pd.perAdd1}}</td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <label>Address 2</label>
                                                        </td>
                                                        <td>{{pd.perAdd2}}</td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <label>Address 3</label>
                                                        </td>
                                                        <td>{{pd.perAdd3}}</td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <label>Address 4</label>
                                                        </td>
                                                        <td>{{pd.perAdd4}}</td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <label>City</label>
                                                        </td>
                                                        <td>{{pd.perCity}}</td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <label>District</label>
                                                        </td>
                                                        <td>{{pd.perDistrict}}</td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <label>State</label>
                                                        </td>
                                                        <td>{{pd.perState}}</td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <label>PinCode</label>
                                                        </td>
                                                        <td>{{pd.perPin}}</td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <label>Contact No.</label>
                                                        </td>
                                                        <td>
                                                            {{pd.perPhone}}
                                                            <br />
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>
                                                            <label>Resident No.</label>
                                                        </td>
                                                        <td></td>
                                                    </tr>
                                                </table>
                                            </td>
                                            @* Present Address *@
                                            <td colspan="2">
                                                <table class="table table-hover table-bordered table-condensed table-striped">
                                                    <tbody>
                                                        <tr>
                                                            <td style="width:35%;">
                                                                <label>House No./Flat No./Room No.</label>
                                                            </td>
                                                            <td style="width:65%;">{{pd.houseNumber}}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <label>Name of Society/Building</label>
                                                            </td>
                                                            <td>{{pd.societyName}}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <label>Name of Area/Location</label>
                                                            </td>
                                                            <td>{{pd.areaName}}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <label>Nearest Landmark</label>
                                                            </td>
                                                            <td>{{pd.landMark}}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <label>Village / City</label>
                                                            </td>
                                                            <td>{{pd.preCity}}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <label>Taluka</label>
                                                            </td>
                                                            <td>{{pd.tehsil}}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <label>Police Station</label>
                                                            </td>
                                                            <td>{{pd.policeStation}}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <label>District</label>
                                                            </td>
                                                            <td>{{pd.preDistrict}}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <label>State</label>
                                                            </td>
                                                            <td>{{pd.preState}}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <label>PinCode</label>
                                                            </td>
                                                            <td>{{pd.prePin}}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <label>Contact No.</label>
                                                            </td>
                                                            <td>{{pd.prePhone}}</td>
                                                        </tr>
                                                        <tr>
                                                            <td>
                                                                <label>Resident No.</label>
                                                            </td>
                                                            <td>{{pd.preResPhone}}</td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </td>
                                        </tr>
                                        @* Documents Details *@
                                        <tr>
                                            <th colspan="4" style="text-align:center;">
                                                <h4>
                                                    <b>Document Details</b>
                                                </h4>
                                            </th>
                                        </tr>
                                        <tr>
                                            <td colspan="4">
                                                <table class="table table-hover table-bordered table-condensed table-striped">
                                                    <tr>
                                                        <td style="width:16.6%">
                                                            <label>{{pd.idPrf3}}</label>
                                                        </td>
                                                        <td style="width:16.6%">
                                                            {{pd.idPrf3No}}
                                                        </td>
                                                        <td style="width:16.6%">
                                                            <label>{{pd.idPrf2}}</label>
                                                        </td>
                                                        <td style="width:16.6%">
                                                            {{pd.idPrf2No}}
                                                        </td>
                                                        <td style="width:16.6%">
                                                            <label>{{pd.idPrf1}}</label>
                                                        </td>
                                                        <td style="width:16.6%">
                                                            {{pd.idPrf1No}}
                                                        </td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </tr>
                                        @* Bank Details *@
                                        @*<tr>
                                                <th colspan="4" style="text-align:center;">
                                                    <h4>
                                                        <b>Bank Details</b>
                                                    </h4>
                                                </th>
                                            </tr>
                                            <tr>
                                                <td colspan="4">
                                                    <table class="table table-hover table-bordered table-condensed table-striped">
                                                        <tr>
                                                            <td style="width:16.6%">
                                                                <label>Bank Account No</label>
                                                            </td>
                                                            <td style="width:16.6%">
                                                                {{pd.bankAcNo}}
                                                            </td>
                                                            <td style="width:16.6%">
                                                                <label>Bank IFSC Code</label>
                                                            </td>
                                                            <td style="width:16.6%">
                                                                {{pd.bankIfsc}}
                                                            </td>
                                                            <td style="width:16.6%">
                                                                <label>Bank Name</label>
                                                            </td>
                                                            <td style="width:16.6%">
                                                                {{pd.bankName}}
                                                            </td>
                                                        </tr>
                                                    </table>
                                                </td>
                                            </tr>*@
                                    </tbody>
                                </table>
                            </div>
                            <div ng-init="GetUserFamInfo()" class="table-responsive">
                                <table class="table table-hover table-bordered table-striped" dir-paginate="f in Ufamdata |itemsPerPage:5">
                                    <thead>
                                        <tr>
                                            <th colspan="5" class="fc-header-center">
                                                <h4>
                                                    <strong>Family Details</strong>
                                                </h4>
                                            </th>
                                        </tr>
                                        <tr>
                                            <th style="text-align:center;">Name</th>
                                            <th style="text-align:center;">Relation</th>
                                            <th style="text-align:center;">Birth Date</th>
                                            <th style="text-align:center;">Occupation</th>
                                            <th style="text-align:center;">Education</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td style="text-align:center;">{{f.name}}</td>
                                            <td style="text-align:center;">{{f.relation}}</td>
                                            <td style="text-align:center;">{{f.birthDt | date:'dd/MM/yyyy'}}</td>
                                            <td style="text-align:center;">{{f.occupation}}</td>
                                            <td style="text-align:center;">{{f.education}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
