@{
    ViewBag.Title = "User Manual";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript">
    $(window).on('load', function () {
        var chk = "@Session["UserRole"]"; var loc = "@Session["Loc"]";
        if (loc === "IPU") { $('#link').text("“http://172.16.12.44:8080”"); $("#url").attr("src", "../../UserManuals/images/LeaveaApplication/IPU.png"); }
        else if (loc === "NKP") { $('#link').text("“http://172.16.10.9:8080”"); $("#url").attr("src", "../../UserManuals/images/LeaveaApplication/NKP.png"); }
        else if (loc === "KJQTL" || loc === "KJSAW") { $('#link').text("“http://172.16.16.77:8080”"); $("#url").attr("src", "../../UserManuals/images/LeaveaApplication/KOSI.png"); };
        if (chk === "1" || chk === "4" || chk === "7" || chk === "9") { $('#btnLA').show(); $('#btnEM').show(); $('#btnLC').show(); $('#btnLR').hide(); $('#btnRS').hide(); $('#btnLP').hide(); }
        if (chk === "2" || chk === "6") { $('#btnLA').show(); $('#btnLR').show(); $('#btnEM').show(); $('#btnLC').show(); $('#btnRS').hide(); $('#btnLP').hide(); }
        if (chk === "3") { $('#btnLA').show(); $('#btnEM').show(); $('#btnRS').show(); $('#btnLC').show(); $('#btnLR').hide(); $('#btnLP').show(); }
        if (chk === "8") { $('#btnLA').show(); $('#btnLR').show(); $('#btnEM').show(); $('#btnRS').show(); $('#btnLC').show(); $('#btnLP').show(); }
    });
    function LoadDiv(data) {
        if (data === "IT") {
            $('#div_IncomeTaxDeclaration').show(); $('#div_LeaveRelease').hide(); $('#div_LeaveApplication').hide(); $('#div_UpdateEmailId').hide();
            $('#div_ReleaseStrategy').hide(); $('#div_LeaveCancellation').hide(); $('#div_LeavePosting').hide();
        }; if (data === "LR") {
            $('#div_LeaveRelease').show(); $('#div_LeaveApplication').hide(); $('#div_UpdateEmailId').hide(); $('#div_ReleaseStrategy').hide();
            $('#div_LeaveCancellation').hide(); $('#div_LeavePosting').hide(); $('#div_IncomeTaxDeclaration').hide();
        }; if (data === "LA") {
            $('#div_LeaveApplication').show(); $('#div_LeaveRelease').hide(); $('#div_UpdateEmailId').hide(); $('#div_ReleaseStrategy').hide();
            $('#div_LeaveCancellation').hide(); $('#div_LeavePosting').hide(); $('#div_IncomeTaxDeclaration').hide();
        }; if (data === "EM") {
            $('#div_UpdateEmailId').show(); $('#div_LeaveRelease').hide(); $('#div_LeaveApplication').hide(); $('#div_ReleaseStrategy').hide();
            $('#div_LeaveCancellation').hide(); $('#div_LeavePosting').hide(); $('#div_IncomeTaxDeclaration').hide();
        }; if (data === "RS") {
            $('#div_ReleaseStrategy').show(); $('#div_LeaveRelease').hide(); $('#div_LeaveApplication').hide(); $('#div_UpdateEmailId').hide();
            $('#div_LeaveCancellation').hide(); $('#div_LeavePosting').hide(); $('#div_IncomeTaxDeclaration').hide();
        }; if (data === "LC") {
            $('#div_LeaveCancellation').show(); $('#div_LeaveRelease').hide(); $('#div_LeaveApplication').hide(); $('#div_UpdateEmailId').hide();
            $('#div_ReleaseStrategy').hide(); $('#div_LeavePosting').hide(); $('#div_IncomeTaxDeclaration').hide();
        }; if (data === "LP") {
            $('#div_LeavePosting').show(); $('#div_LeaveRelease').hide(); $('#div_LeaveApplication').hide(); $('#div_UpdateEmailId').hide();
            $('#div_ReleaseStrategy').hide(); $('#div_LeaveCancellation').hide(); $('#div_IncomeTaxDeclaration').hide();
        };
    };
</script>
<div class="container">
    <div class="row" id="div_Main">
